<% content_for :ready_js do %>
  NoCMS.Admin.BlockHandler();
<% end %>

<% content_for :second_menu do %>
  <%= render partial: 'menu_listing_item', collection: @menus, as: 'menu', locals: {current: @menu } %>
<% end unless @menus.blank? %>

<%= no_cms_admin_form_for @menu do |f| %>
  <%= render partial: 'no_cms/admin/shared/toolbar', locals: { f: f } %>
  <div class="content">
    <%= render 'no_cms/admin/shared/log_messages' %>
    <%= render 'content_fields', f: f %>
    <%= render 'no_cms/admin/menus/menu_items/index', f: f %>
  </div>

<% end %>
